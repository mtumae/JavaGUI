<?xml version="1.0" encoding="UTF-8"?>

        <?import javafx.geometry.Insets?>
        <?import javafx.scene.control.Label?>
        <?import javafx.scene.layout.VBox?>


        <?import javafx.scene.control.Button?>
        <?import javafx.scene.layout.HBox?>
        <?import javafx.scene.text.Text?>
<?import javafx.scene.control.TextField?>
<VBox alignment="CENTER" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.mutere.sufeeds.sufeedsproject.Signin">
<padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
</padding>
    <Label style="-fx-font-size: 25;">Sign in</Label>
<VBox spacing="5.0" style="-fx-background-radius: 20; -fx-border-radius: 20; -fx-background-color: #e0e0e0;-fx-border-color: #e0e0e0; -fx-border-width: 1px; -fx-border-style: solid; -fx-text-fill: white;" maxWidth="300">
    <padding>
        <Insets right="20.0" bottom="20.0" left="20.0" top="20.0"/>
    </padding>
    <Label style="-fx-font-size: 15;">Admission Number</Label>
    <TextField fx:id="ad_no"></TextField>
    <Label style="-fx-font-size: 15;">Password</Label>
    <TextField fx:id="pass"></TextField>

    <VBox alignment="CENTER">
        <Button style="-fx-background-color: #e0e0e0" onAction="#switchtoRegister" maxWidth="300">Dont have an account? Sign up</Button>
        <Button onAction="#SigninButtonClick" maxWidth="300">Sign in</Button>
    </VBox>



</VBox>
</VBox>


