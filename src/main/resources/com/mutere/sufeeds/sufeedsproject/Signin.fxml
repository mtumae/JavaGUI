<?xml version="1.0" encoding="UTF-8"?>

        <?import javafx.geometry.Insets?>
        <?import javafx.scene.control.Label?>
        <?import javafx.scene.layout.VBox?>


        <?import javafx.scene.control.Button?>
        <?import javafx.scene.layout.HBox?>
        <?import javafx.scene.text.Text?>
<?import javafx.scene.control.TextField?>
<VBox alignment="CENTER"  spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.mutere.sufeeds.sufeedsproject.Signin">
<padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
</padding>

<VBox style="-fx-background-color: #e0e0e0;-fx-border-color: black; -fx-border-width: 2px; -fx-border-style: solid; -fx-text-fill: white;" maxWidth="300">
    <padding>
        <Insets bottom="20.0" left="20.0" top="20.0"/>
    </padding>
    <Label>Admission Number</Label>
    <TextField fx:id="ad_no" maxWidth="200" ></TextField>
    <Label>Password</Label>
    <TextField fx:id="pass1" maxWidth="200" ></TextField>
    <Label>Password confirmation</Label>
    <TextField fx:id="pass2" maxWidth="200" ></TextField>
    <Button onAction="#SigninButtonClick">Sign in</Button>
</VBox>
</VBox>


